<% layout('layouts/boilerplate')%>


  <div class="container">
    <h1>
        Standings
    </h1>
    <div class="container">
      <div class="row justify-content-center gx-5">
        <h1><%= standings.league.name %></h1>
        <h2><%= standings.league.country %></h2>
        <img src="<%= standings.league.flag%>" width="50px" height="50px" alt="">
        <div class="col-auto">
          <button class="btn btn-outline-info" id="prevMonthBtn">이전 시즌</button>
        </div>
        <div class="col-auto">
          <div id="year"><%= standings.league.season %>시즌 순위표</div>
        </div>
        <div class="col-auto">
          <button class="btn btn-outline-info" id="nextMonthBtn">다음 시즌 </button>
        </div>
      </div>
    </div>
    <div class="standingContainer">

    </div>

  </div>

  <script src="/js/standingScripts.js"> </script>
  </div>